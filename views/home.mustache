				<div class="row m-top-4 section-list">
					<div class="col-md-4 col-sm-12">
						<div class="section-box">
							<h4 class="white-font align-center">{{page.section8.lists.list1.title}}</h4>
							<p>{{page.section8.lists.list1.payment}}</p>
							<ul>
								<li>{{page.section8.lists.list1.payment1}}</li>
								<li>{{page.section8.lists.list1.payment2}}</li>
							</ul>
							<p>{{page.section8.lists.list1.sched}}</p>
							<p>{{page.section8.lists.list1.topics}}</p>
							<p>{{page.section8.lists.list1.time}}</p>
							<p>{{page.section8.lists.list1.date}}</p>
						</div>
					</div>
					<div class="col-md-4 col-sm-12">
						<div class="section-box">
							<h4 class="white-font align-center">{{page.section8.lists.list2.title}}</h4>
